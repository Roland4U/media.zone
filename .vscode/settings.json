{
    "git.ignoreLimitWarning": true,
    "python.languageServer": "Jedi",
    "sqltools.connections": [
        {
            "database": "${workspaceFolder:media.zone}/app/db.sqlite3",
            "driver": "SQLite",
            "name": "app",
            "previewLimit": 50
        }
    ],
    "sqltools.useNodeRuntime": true
}
